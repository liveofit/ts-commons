{
  "name": "@liveofit/ts-commons",
  "version": "1.1.21",
  "description": "Common functions to use across all typescript projects",
  "scripts": {
    "rollup": "rollup -c",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "prettier:check": "prettier --check --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "prettier:format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "NODE_ENV=\"\" jest",
    "test:coverage": "jest --coverage",
    "typedoc:generate": "typedoc --entryPointStrategy expand ./src",
    "generate-changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s"
  },
  "author": "YOUR_NAME",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.6.4"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "colors": "^1.4.0"
  }
}
